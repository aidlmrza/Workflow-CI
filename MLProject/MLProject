name: diabetes-mlflow-project

entry_points:
  main:
    parameters:
      data_path:
        type: str
        default: dataset_diabetes.csv  # Ganti dengan dataset yang Anda gunakan
      target_col:
        type: str
        default: target  # Ganti dengan kolom target yang sesuai

    command: >
      python modelling.py
      --data_path {data_path}
      --target_col {target_col}
